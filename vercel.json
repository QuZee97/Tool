{
  "functions": {
    "api/pdf.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/tool", "destination": "/tool.html" },
    { "source": "/dashboard", "destination": "/dashboard.html" }
  ],
  "redirects": [
    { "source": "/tool.html", "permanent": true, "destination": "/tool" },
    { "source": "/dashboard.html", "permanent": true, "destination": "/dashboard" }
  ]
}
